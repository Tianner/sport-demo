<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="babasport-protal"/>
   <!-- <dubbo:registry address="192.168.57.104:2181" protocol="zookeeper"/>-->
    <dubbo:registry address="N/A"/>

    <dubbo:reference interface="com.sunwoda.babasport.service.SolrService" id="solrService" timeout="60000" url="dubbo://127.0.0.1:20881"/>

    <dubbo:reference interface="com.sunwoda.babasport.service.ProductService" id="productService" timeout="60000" url="dubbo://127.0.0.1:20880"/>

    <!--设置不检查生产者-->
    <dubbo:consumer timeout="60000" check="false"/>
</beans>